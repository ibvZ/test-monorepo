{
  "name": "ibvz/monorepo",
  "description": "A PHP monorepo",
  "type": "project",
  "repositories": [
    {
      "type": "path",
      "url": "packages/*",
      "options": {
        "symlink": true
      }
    }
  ],
  "require": {
    "php": ">=8.2",
    "ibvz/objects": "@dev",
    "ibvz/users": "@dev"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
{
  "name": "ibvz/monorepo",
  "require": {
    "php": "^8.0"
  },
  "replace": {
    "packages/objects": "self.version",
    "packages/users": "self.version"
  },

  "autoload": {
    "psr-4": {
      "Packages\\": "packages/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.x-dev"
    }
  },
  "config": {
    "sort-packages": true,
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
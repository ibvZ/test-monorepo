{
  "name": "ibvz/monorepo",
  "description": "Монорепозиторий для пакетов",
  "type": "project",
  "repositories": [
    {
      "type": "git",
      "url": "https://github.com/ibvZ/test-monorepo.git"
    },
    {
      "type": "path",
      "url": "./*",
      "options": {
        "symlink": true
      }
    }
  ],
  "require": {
    "ibvz/objects": "master",
    "ibvz/users": "master"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "optimize-autoloader": true,
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "extra": {
    "branch-alias": {
      "master": "1.0.x"
    }
  }
}
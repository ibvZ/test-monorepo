{
  "name": "ibvz/monorepo",
  "require": {
    "php": "^8.0",
    "packages/objects": "^1.0.9",
    "packages/users": "^1.0.9"
  },
  "repositories": [
    {
      "type": "path",
      "url": "./packages/*",
      "options": {
        "symlink": true
      }
    }
  ],
  "autoload": {
    "psr-4": {
      "Packages\\": "packages/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "preferred-install": {
      "*": "source"
    }
  }
}